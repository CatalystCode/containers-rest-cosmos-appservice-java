{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [false],
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "indent": [true, "spaces", 4],
        "ordered-imports": [
            true,
            {
                "grouped-imports": true
            }
        ]
    },
    "rulesDirectory": []
}
